   <parameter CLSNAME="ZCL_BANCO_00" CMPNAME="RETORNA_LIMITE_TOTAL" SCONAME="RV_LIMITE_TOTAL" VERSION="1" LANGU="P" DESCRIPT="RV_LIMITE_TOTAL" CMPTYPE="1" MTDTYPE="0" EDITORDER="5 " DISPID="0 " PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="1" TYPE="DEC_16_02_S"/>
   <source>method RETORNA_LIMITE_TOTAL.

    DATA: R_CORRENTE TYPE REF TO ZCL_CONTA_CORRENTE_00.

    LOOP AT CONTA_LISTA INTO DATA(R_CONTA).
      TRY.
        R_CORRENTE ?= R_CONTA.
        RV_LIMITE_TOTAL = RV_LIMITE_TOTAL + R_CORRENTE-&gt;RETORNA_LIM_CHE( ).

      CATCH CX_SY_MOVE_CAST_ERROR.
      ENDTRY.
    ENDLOOP.

  endmethod.</source>
  </method>
