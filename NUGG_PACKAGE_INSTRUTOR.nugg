 <PROG NAME="ZIFI_CLASSES_00" VARCL="X" SUBC="I" RMAND="800" RLOAD="P" UCCHECK="X">
  <textPool>
   <language SPRAS="P">
    <textElement ID="R" ENTRY="Include ZIFI_CLASSES_00" LENGTH="23 "/>
   </language>
  </textPool>
  <source>*&amp;---------------------------------------------------------------------*
*&amp;  Include           ZIFI_CLASSES_00
*&amp;---------------------------------------------------------------------*

CLASS LCL_CONTA DEFINITION.
  PUBLIC SECTION.
    METHODS: CONSTRUCTOR IMPORTING IM_TITULAR TYPE STRING
                                   IM_SALDO   TYPE DEC_16_02_S.

    METHODS: DEFINE_ATRIBUTOS IMPORTING IM_TITULAR TYPE STRING
                                        IM_SALDO   TYPE DEC_16_02_S.

    METHODS: RETORNA_SALDO RETURNING VALUE(RV_SALDO) TYPE DEC_16_02_S
                           RAISING ZCX_CONTA_BLOQUEADA_02.

    METHODS: IMPRIME_ATRIBUTOS.

    CLASS-METHODS: RETORNA_NUM_CONTAS RETURNING VALUE(RV_NUM_CONTAS) TYPE I.

*-- Passo 1 - Declaração do evento
    EVENTS: conta_criada.

  PRIVATE SECTION.
    DATA: TITULAR TYPE STRING,
          SALDO   TYPE DEC_16_02_S.
    CLASS-DATA: NUM_CONTAS TYPE I.
ENDCLASS.

CLASS LCL_CONTA IMPLEMENTATION.
   METHOD CONSTRUCTOR.
     ME-&gt;SALDO = IM_SALDO. &quot;A utilização do ME é opcional.
     TITULAR = IM_TITULAR.

     ADD 1 TO NUM_CONTAS.

*-- Passo 2 - Disparar o evento
     RAISE EVENT conta_criada.
   ENDMETHOD.

   METHOD DEFINE_ATRIBUTOS.
     ME-&gt;SALDO = IM_SALDO. &quot;A utilização do ME é opcional.
     TITULAR = IM_TITULAR.
   ENDMETHOD.

   METHOD RETORNA_SALDO.
     IF SALDO LE 0.
       RAISE EXCEPTION TYPE ZCX_CONTA_BLOQUEADA_02.
     ENDIF.

     RV_SALDO = SALDO.
   ENDMETHOD.

   METHOD IMPRIME_ATRIBUTOS.
     WRITE: / &apos;TITULAR: &apos;, TITULAR,
              027 &apos;SALDO:&apos;, SALDO.
   ENDMETHOD.

   METHOD RETORNA_NUM_CONTAS.
     RV_NUM_CONTAS = NUM_CONTAS.
   ENDMETHOD.
ENDCLASS.

CLASS LCL_CONTA_CORRENTE DEFINITION
                         INHERITING FROM LCL_CONTA.
  PUBLIC SECTION.
    METHODS: CONSTRUCTOR IMPORTING IMC_TITULAR TYPE STRING
                                   IMC_SALDO TYPE DEC_16_02_S
                                   IM_LIM_CHE TYPE DEC_16_02_S.

    METHODS: RETORNA_SALDO REDEFINITION.

    METHODS: RETORNA_LIM_CHE RETURNING VALUE(RV_LIM_CHE) TYPE DEC_16_02_S.

  PRIVATE SECTION.
    DATA: LIM_CHE TYPE DEC_16_02_S.
ENDCLASS.

CLASS LCL_CONTA_CORRENTE IMPLEMENTATION.
  METHOD CONSTRUCTOR.
    SUPER-&gt;CONSTRUCTOR(
      EXPORTING
        IM_TITULAR = IMC_TITULAR
        IM_SALDO   = IMC_SALDO ).

    LIM_CHE = IM_LIM_CHE.
  ENDMETHOD.

  METHOD RETORNA_SALDO.
    RV_SALDO = SUPER-&gt;RETORNA_SALDO( ) + LIM_CHE.
  ENDMETHOD.

  METHOD RETORNA_LIM_CHE.
    RV_LIM_CHE = LIM_CHE.
  ENDMETHOD.
ENDCLASS.

CLASS LCL_CONTA_POUPANCA DEFINITION
                         INHERITING FROM LCL_CONTA.
   PUBLIC SECTION.
     METHODS: CONSTRUCTOR IMPORTING IMC_TITULAR TYPE STRING
                                    IMC_SALDO TYPE DEC_16_02_S
                                    IM_TAXA_JU TYPE DEC_16_02_S.

     METHODS: RETORNA_SALDO REDEFINITION.

     METHODS: RETORNA_TAXA_JU RETURNING VALUE(RV_TAXA_JU) TYPE DEC_16_02_S.

   PRIVATE SECTION.
     DATA: TAXA_JU TYPE DEC_16_02_S.
ENDCLASS.

CLASS LCL_CONTA_POUPANCA IMPLEMENTATION.
   METHOD CONSTRUCTOR.
     SUPER-&gt;CONSTRUCTOR(
       exporting
         IM_TITULAR = IMC_TITULAR
         IM_SALDO   = IMC_SALDO ).

     TAXA_JU = IM_TAXA_JU.
   ENDMETHOD.

   METHOD RETORNA_SALDO.
     RV_SALDO = SUPER-&gt;RETORNA_SALDO( ) * ( 1 + TAXA_JU ).
   ENDMETHOD.

   METHOD RETORNA_TAXA_JU.
     RV_TAXA_JU = TAXA_JU.
   ENDMETHOD.
 ENDCLASS.

 CLASS LCL_BANCO DEFINITION.
  PUBLIC SECTION.
    METHODS: constructor.

*-- Passo 3 - Definir qual método vai tratar o evento
    METHODS: ADICIONA_CONTA FOR EVENT conta_criada of lcl_conta
                            IMPORTING sender.

    METHODS: RETORNA_SALDO_TOTAL RETURNING VALUE(RV_SALDO_TOTAL) TYPE DEC_16_02_S.

    METHODS: RETORNA_LIMITE_TOTAL RETURNING VALUE(RV_LIMITE_TOTAL) TYPE DEC_16_02_S.
  PRIVATE SECTION.
    DATA: CONTA_LISTA TYPE TABLE OF REF TO LCL_CONTA. &quot;Para criar a lista de referências;
                                                     &quot;LCL_CONTA pois dessa forma, é possível criar tanto CONTA_CORRENTE quando CONTA_POUPANCA
ENDCLASS.

CLASS LCL_BANCO IMPLEMENTATION.
  METHOD constructor.
*-- Passo 4 - Registrar o evento em tempo de execução
    SET HANDLER ADICIONA_CONTA FOR ALL INSTANCES.
  ENDMETHOD.


  METHOD ADICIONA_CONTA.
    APPEND sender TO CONTA_LISTA.
  ENDMETHOD.

  METHOD RETORNA_SALDO_TOTAL.
    LOOP AT CONTA_LISTA INTO DATA(R_CONTA). &quot;NOVA FORMA DE DECLARAR A VARIÁVEL NO ABAP = IN LINE DECLARATION
      RV_SALDO_TOTAL = RV_SALDO_TOTAL + R_CONTA-&gt;RETORNA_SALDO( ).
    ENDLOOP.
  ENDMETHOD.

  METHOD RETORNA_LIMITE_TOTAL.
    DATA: R_CORRENTE TYPE REF TO LCL_CONTA_CORRENTE.

    LOOP AT CONTA_LISTA INTO DATA(R_CONTA).
      TRY.
        R_CORRENTE ?= R_CONTA.
        RV_LIMITE_TOTAL = RV_LIMITE_TOTAL + R_CORRENTE-&gt;RETORNA_LIM_CHE( ).

      CATCH CX_SY_MOVE_CAST_ERROR.
      ENDTRY.
    ENDLOOP.
  ENDMETHOD.
ENDCLASS.</source>
 </PROG>
