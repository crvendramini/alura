*&amp;---------------------------------------------------------------------*
*&amp;
*&amp;---------------------------------------------------------------------*
REPORT zrca_alv_simples_00.


CLASS lcl_handler DEFINITION.
  PUBLIC SECTION.
    METHODS: on_double_click FOR EVENT double_click OF cl_gui_alv_grid
      IMPORTING e_column es_row_no.
    DATA:
      wa_sflight TYPE sflight,
      t_sflight  TYPE TABLE OF sflight.
ENDCLASS.

CLASS lcl_handler IMPLEMENTATION.
  METHOD on_double_click.
*    MESSAGE i888(sabapdocu) WITH &apos;Você deu o duplo clique no campo &apos;
*    e_column &apos; e linha &apos;
*    es_row_no-row_id.

    READ TABLE t_sflight INTO wa_sflight INDEX es_row_no-row_id.
*    IF sy-subrc = 0.
*      MESSAGE i888(sabapdocu) WITH wa_sflight-carrid wa_sflight-connid.
*    ENDIF.
    IF sy-subrc = 0.
      SUBMIT zsel_single_00
              WITH p_carrid = wa_sflight-carrid
              WITH p_connid = wa_sflight-connid
              AND RETURN.
    ENDIF.


  ENDMETHOD. &quot;on_double_click

ENDCLASS.


DATA: r_container TYPE REF TO cl_gui_custom_container,
      r_grid      TYPE REF TO cl_gui_alv_grid,
      r_handler   TYPE REF TO lcl_handler.

DATA: t_sflight TYPE TABLE OF sflight.

START-OF-SELECTION.
  CREATE OBJECT r_handler.

  SET HANDLER r_handler-&gt;on_double_click FOR ALL INSTANCES.

  SELECT *
    INTO TABLE t_sflight
    FROM sflight.

  r_handler-&gt;t_sflight = t_sflight.
  IF sy-subrc = 0.

    CALL SCREEN 0100.
  ENDIF.

  INCLUDE zica_alv_simples_00_o01.

  INCLUDE zica_alv_simples_00_i01.</source>
