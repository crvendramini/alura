 <PROG NAME="SAPMZ00B_I01" VARCL="X" SUBC="I" LEVL="750" RMAND="800" RLOAD="P" UCCHECK="X">
  <textPool>
   <language SPRAS="P">
    <textElement ID="R" ENTRY="Include SAPMZ00A_I01" LENGTH="20 "/>
   </language>
  </textPool>
  <source>*----------------------------------------------------------------------*
***INCLUDE SAPMZ00A_I01.
*----------------------------------------------------------------------*
*&amp;---------------------------------------------------------------------*
*&amp;      Module  GET_SFLIGHT  INPUT
*&amp;---------------------------------------------------------------------*
*       text
*----------------------------------------------------------------------*
MODULE GET_SFLIGHT INPUT.
  SELECT SINGLE *
    INTO WA_SFLIGHT
    FROM SFLIGHT
    WHERE CARRID = SDYN_CONN-CARRID
      AND CONNID = SDYN_CONN-CONNID
      AND FLDATE = SDYN_CONN-FLDATE.

  IF SY-SUBRC NE 0.
    CLEAR: WA_SFLIGHT.
    MESSAGE E007(BC410).
  ENDIF.
ENDMODULE.

*&amp;---------------------------------------------------------------------*
*&amp;      Module  USER_COMMAND_0100  INPUT
*&amp;---------------------------------------------------------------------*
*       text
*----------------------------------------------------------------------*
MODULE USER_COMMAND_0100 INPUT.
  CASE OK_CODE.
    WHEN &apos;BACK&apos;.
      LEAVE TO SCREEN 0.

    WHEN &apos;SAVE&apos;.
      UPDATE SFLIGHT FROM WA_SFLIGHT.

      IF SY-SUBRC NE 0.
        MESSAGE A008(BC410).
      ENDIF.

      MESSAGE S009(BC410).

    WHEN &apos;FC1&apos; OR &apos;FC2&apos; OR &apos;FC3&apos;.
      MY_TAB_STRIP-ACTIVETAB = OK_CODE.
  ENDCASE.
ENDMODULE.

*&amp;---------------------------------------------------------------------*
*&amp;      Module  EXIT_PROGRAM  INPUT
*&amp;---------------------------------------------------------------------*
*       text
*----------------------------------------------------------------------*
MODULE EXIT_PROGRAM INPUT.
  CASE OK_CODE.
    WHEN &apos;CANCEL&apos;.
      CLEAR: WA_SFLIGHT.
      SET PARAMETER ID &apos;CAR&apos; FIELD WA_SFLIGHT-CARRID.
      SET PARAMETER ID &apos;CON&apos; FIELD WA_SFLIGHT-CONNID.
      SET PARAMETER ID &apos;DAY&apos; FIELD WA_SFLIGHT-FLDATE.
      LEAVE TO SCREEN 0100.

    WHEN &apos;EXIT&apos;.
      LEAVE PROGRAM.
  ENDCASE.
ENDMODULE.

*&amp;---------------------------------------------------------------------*
*&amp;      Module  GET_SAPLANE  INPUT
*&amp;---------------------------------------------------------------------*
*       text
*----------------------------------------------------------------------*
MODULE GET_SAPLANE INPUT.
  SELECT SINGLE *
    INTO WA_SAPLANE
    FROM SAPLANE
    WHERE PLANETYPE = SDYN_CONN-PLANETYPE.

  IF SY-SUBRC NE 0.
    MESSAGE &apos;TIPO DE AVIÃO NÃO ENCONTRADO&apos; TYPE &apos;E&apos;.
  ENDIF.

  IF SDYN_CONN-SEATSOCC GT WA_SAPLANE-SEATSMAX.
    MESSAGE E109(BC410).

  ELSE.
    WA_SFLIGHT-PLANETYPE = WA_SAPLANE-PLANETYPE.
    WA_SFLIGHT-SEATSMAX = WA_SAPLANE-SEATSMAX.
  ENDIF.
ENDMODULE.

*&amp;---------------------------------------------------------------------*
*&amp;      Module  MODIFY_ITAB  INPUT
*&amp;---------------------------------------------------------------------*
*       text
*----------------------------------------------------------------------*
MODULE MODIFY_ITAB INPUT.
  MOVE-CORRESPONDING SDYN_BOOK TO WA_BOOK.
  MODIFY T_BOOK FROM WA_BOOK
    INDEX MY_CONTROL-CURRENT_LINE.
ENDMODULE.</source>
 </PROG>
 <PROG NAME="SAPMZ00B_O01" VARCL="X" SUBC="I" LEVL="750" RMAND="800" RLOAD="P" UCCHECK="X">
  <textPool>
   <language SPRAS="P">
    <textElement ID="R" ENTRY="Include SAPMZ00A_O01" LENGTH="20 "/>
   </language>
  </textPool>
  <source>*----------------------------------------------------------------------*
***INCLUDE SAPMZ00A_O01.
*----------------------------------------------------------------------*
*&amp;---------------------------------------------------------------------*
*&amp;      Module  FILL_SCREEN  OUTPUT
*&amp;---------------------------------------------------------------------*
*       text
*----------------------------------------------------------------------*
MODULE FILL_SCREEN OUTPUT.
  MOVE-CORRESPONDING WA_SFLIGHT TO SDYN_CONN.
ENDMODULE.

*&amp;---------------------------------------------------------------------*
*&amp;      Module  STATUS_0100  OUTPUT
*&amp;---------------------------------------------------------------------*
*       text
*----------------------------------------------------------------------*
MODULE STATUS_0100 OUTPUT.
  REFRESH: T_BUTTON.

  CASE MY_TAB_STRIP-ACTIVETAB.
    WHEN &apos;FC1&apos;.
      APPEND &apos;SAVE&apos; TO T_BUTTON.
*      SET PF-STATUS &apos;STATUS_0100&apos; EXCLUDING &apos;SAVE&apos;.
      SET TITLEBAR &apos;TITLE_0100&apos; WITH TEXT-VIE.

    WHEN &apos;FC2&apos;.
*      SET PF-STATUS &apos;STATUS_0100&apos;.
      SET TITLEBAR &apos;TITLE_0100&apos; WITH TEXT-FLI.

    WHEN &apos;FC3&apos;.
      APPEND &apos;SAVE&apos; TO T_BUTTON.
*      SET PF-STATUS &apos;STATUS_0100&apos; EXCLUDING &apos;SAVE&apos;.
      SET TITLEBAR &apos;TITLE_0100&apos; WITH TEXT-BOO.

    WHEN OTHERS.
      APPEND &apos;SAVE&apos; TO T_BUTTON.
*      SET PF-STATUS &apos;STATUS_0100&apos; EXCLUDING &apos;SAVE&apos;.
      SET TITLEBAR &apos;TITLE_0100&apos; WITH TEXT-VIE.
  ENDCASE.

  SET PF-STATUS &apos;STATUS_0100&apos; EXCLUDING T_BUTTON.

  CLEAR: OK_CODE.
ENDMODULE.

*&amp;---------------------------------------------------------------------*
*&amp;      Module  MODIFY_SCREEN  OUTPUT
*&amp;---------------------------------------------------------------------*
*       text
*----------------------------------------------------------------------*
MODULE MODIFY_SCREEN OUTPUT.
    LOOP AT SCREEN.
      IF SCREEN-NAME = &apos;SDYN_CONN-PLANETYPE&apos;.
        IF MY_TAB_STRIP-ACTIVETAB = &apos;FC2&apos;.
          SCREEN-INPUT = 1.

        ELSE.
          SCREEN-INPUT = 0.
        ENDIF.

        MODIFY SCREEN.
      ENDIF.
    ENDLOOP.
ENDMODULE.

*&amp;---------------------------------------------------------------------*
*&amp;      Module  DEFINE_SUB_SCREEN  OUTPUT
*&amp;---------------------------------------------------------------------*
*       text
*----------------------------------------------------------------------*
MODULE DEFINE_SUB_SCREEN OUTPUT.
  CASE MY_TAB_STRIP-ACTIVETAB.
    WHEN &apos;FC1&apos;.
      DYNNR = 0110.

    WHEN &apos;FC2&apos;.
      DYNNR = 0120.

    WHEN &apos;FC3&apos;.
      DYNNR = 0130.

    WHEN OTHERS.
      DYNNR = 0110.
      MY_TAB_STRIP-ACTIVETAB = &apos;FC1&apos;.
  ENDCASE.
ENDMODULE.

*&amp;---------------------------------------------------------------------*
*&amp;      Module  GET_SPFLI  INPUT
*&amp;---------------------------------------------------------------------*
*       text
*----------------------------------------------------------------------*
MODULE GET_SPFLI OUTPUT.
  ON CHANGE OF WA_SFLIGHT-CARRID
            OR WA_SFLIGHT-CONNID.
    SELECT SINGLE *
      INTO WA_SPFLI
      FROM SPFLI
      WHERE CARRID = WA_SFLIGHT-CARRID
        AND CONNID = WA_SFLIGHT-CONNID.

    IF SY-SUBRC EQ 0.
      MOVE-CORRESPONDING WA_SPFLI TO SDYN_CONN.
    ENDIF.
  ENDON.
ENDMODULE.

*&amp;---------------------------------------------------------------------*
*&amp;      Module  GET_AIRCRAFT  INPUT
*&amp;---------------------------------------------------------------------*
*       text
*----------------------------------------------------------------------*
MODULE GET_AIRCRAFT OUTPUT.
  ON CHANGE OF WA_SFLIGHT-PLANETYPE.
    SELECT SINGLE *
      INTO WA_SAPLANE
      FROM SAPLANE
      WHERE PLANETYPE = WA_SFLIGHT-PLANETYPE.

    IF SY-SUBRC = 0.
      MOVE: WA_SAPLANE TO SAPLANE.
    ENDIF.
  ENDON.
ENDMODULE.

*&amp;---------------------------------------------------------------------*
*&amp;      Module  GET_SBOOK  OUTPUT
*&amp;---------------------------------------------------------------------*
*       text
*----------------------------------------------------------------------*
MODULE GET_SBOOK OUTPUT.
  ON CHANGE OF WA_SFLIGHT-CARRID
            OR WA_SFLIGHT-CONNID
            OR WA_SFLIGHT-FLDATE.
    SELECT *
      INTO CORRESPONDING FIELDS OF TABLE T_BOOK
      FROM SBOOK
      WHERE CARRID = WA_SFLIGHT-CARRID
        AND CONNID = WA_SFLIGHT-CONNID
        AND FLDATE = WA_SFLIGHT-FLDATE.
  ENDON.
ENDMODULE.

*&amp;---------------------------------------------------------------------*
*&amp;      Module  MOVE_TO_TC  OUTPUT
*&amp;---------------------------------------------------------------------*
*       text
*----------------------------------------------------------------------*
MODULE MOVE_TO_TC OUTPUT.
  MOVE-CORRESPONDING WA_BOOK TO SDYN_BOOK.
ENDMODULE.</source>
 </PROG>
