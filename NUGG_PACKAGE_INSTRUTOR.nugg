 <PROG NAME="ZIFI_INTERFACES_00" VARCL="X" SUBC="I" RMAND="800" RLOAD="P" UCCHECK="X">
  <textPool>
   <language SPRAS="P">
    <textElement ID="R" ENTRY="Include ZIFI_INTERFACES_00" LENGTH="26 "/>
   </language>
  </textPool>
  <source>*&amp;---------------------------------------------------------------------*
*&amp;  Include           ZIFI_INTERFACES_00
*&amp;---------------------------------------------------------------------*

INTERFACE lif_relatorio.
  METHODS: selecionar_dados IMPORTING im_codigo TYPE char10.

  METHODS: imprime_dados.

ENDINTERFACE.

CLASS lcl_cliente DEFINITION.
  PUBLIC SECTION.
    INTERFACES: lif_relatorio.

  PRIVATE SECTION.
    data: wa_kna1 type kna1.
endclass.

CLASS lcl_cliente IMPLEMENTATION.
  METHOD lif_relatorio~selecionar_dados.
    DATA: lv_kunnr TYPE kna1-kunnr.

    lv_kunnr = im_codigo.

    select SINGLE *
      INTO wa_kna1
      FROM kna1
      WHERE kunnr = lv_kunnr.

 ENDMETHOD.

 METHOD lif_relatorio~imprime_dados.
  WRITE: / wa_kna1-kunnr,
           wa_kna1-name1.
 ENDmethod.
ENDCLASS.</source>
 </PROG>
 <PROG NAME="ZITAB_00" VARCL="X" SUBC="1" RMAND="800" RLOAD="P" FIXPT="X" UCCHECK="X">
  <textPool>
   <language SPRAS="P">
    <textElement ID="R" ENTRY="Programa ZITAB_00" LENGTH="17 "/>
   </language>
  </textPool>
  <source>*&amp;---------------------------------------------------------------------*
*&amp; Report ZITAB_00
*&amp;---------------------------------------------------------------------*
*&amp;
*&amp;---------------------------------------------------------------------*
REPORT ZITAB_00.

TYPES: BEGIN OF TYP_FLIGHT,
         CARRID TYPE SFLIGHT-CARRID,
         CONNID TYPE SFLIGHT-CONNID,
         FLDATE TYPE SFLIGHT-FLDATE,
         PRICE  TYPE SFLIGHT-PRICE,
         CURRENCY TYPE SFLIGHT-CURRENCY,
         SEATSOCC TYPE SFLIGHT-SEATSOCC,
       END OF TYP_FLIGHT.

DATA: WA_SFLIGHT TYPE TYP_FLIGHT.
DATA: IT_SFLIGHT TYPE STANDARD TABLE OF TYP_FLIGHT.

PARAMETERS: P_CARRID TYPE SFLIGHT-CARRID,
            P_CONNID TYPE SFLIGHT-CONNID.

SELECT CARRID CONNID FLDATE PRICE CURRENCY SEATSOCC
  INTO TABLE IT_SFLIGHT
  FROM SFLIGHT
  WHERE CARRID = P_CARRID
    AND CONNID = P_CONNID.

IF SY-SUBRC = 0.
  LOOP AT IT_SFLIGHT INTO WA_SFLIGHT.
    WRITE: / WA_SFLIGHT-CARRID,
             WA_SFLIGHT-CONNID,
             WA_SFLIGHT-FLDATE,
             WA_SFLIGHT-PRICE,
             WA_SFLIGHT-CURRENCY,
             WA_SFLIGHT-SEATSOCC.
  ENDLOOP.

ELSE.
  WRITE: &apos;DADOS NÃO ENCONTRADOS&apos;.
ENDIF.</source>
 </PROG>
